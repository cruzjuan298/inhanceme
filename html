                    <div>
                        <section class="sections-survey" id="Guide">
                        <h1 id-="titlea">Find out what to feed your soul...<h1>
                        <form id="survey-form">
                            <p id="moodtext">What emoition are you feeling right now?</p>
                            <input type="radio" name="mood" value="sad"> Sad<br>
                            <input type="radio" name="mood" value="mad"> Mad<br>
                            <input type="radio" name="mood" value="enpowered"> Enpowered<br>
                            <input type="radio" name="mood" value="fear"> Fear<br>
                          </form>
                          
                          <div id="video-container"></div>
                          
                            // Get the survey form and video container elements
                            const surveyForm = document.getElementById('survey-form');
                            const videoContainer = document.getElementById('video-container');
                          
                            // Listen for a change in the form's input fields
                            surveyForm.addEventListener('change', (event) => {
                              // Users answer is taken
                              const selectedAnswer = event.target.value;
                          
                              // Displays the correct video based on the persons answer
                              if (selectedAnswer === 'sad') {
                                videoContainer.innerHTML = '<iframe src="https://www.youtube.com/embed/1Dk04sX-qCE" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>';
                              } else if (selectedAnswer === 'mad') {
                                videoContainer.innerHTML = '<iframe src="https://www.youtube.com/embed/npCwIUiJS4s" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>';
                              } else if (selectedAnswer === 'enpowered') {
                                videoContainer.innerHTML = '<iframe src="https://www.youtube.com/embed/yiGweP--BRs" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>';
                              } else if (selectedAnswer === 'fear') {
                                videoContainer.innerHTML = '<iframe src="https://www.youtube.com/embed/LU_lEl-n5Ec" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>';
                              }
                            });
                        </section>
